{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Launch Program",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/.next/**/*.js",
      ],
      "port": 9229,
      "sourceMapPathOverrides": {
          "webpack://_N_E/*": "${webRoot}/*"
      }
    }
  ]
}

